<title>Employee</title>
<table width="90%" border="0" cellpadding="0" cellspacing="0" align ="center" >

	<tr>
		<td><div class ="fontHeading " align = "center"><h1>My Profile</h1></div></td>
	</tr>
	
	<tr>
		<td>
			<div class = "textfont" align ="right"><%= link_to "Edit My Profile"  , url_for({:controller => 'employee', :action => 'editMyProfile' , :prospect => session[:userid].to_s }) %></div>
		</td>
	</tr>

	<tr>
		<td>
			<div class = "textfont">
				<% unless(@dataARY.nil?)%>
					<% @dataARY.each do |value| %>
						<table align = "center">
							<tr>
								<td>First Name</td> 
								<td><%= value['firstname'] %></td>
							</tr>
							<tr>
								<td>Last Name</td> 
								<td><%= value['lastname'] %></td>
							</tr>
							<tr>
								<td>Address</td> 
								<td><%= value['address'] %></td>
							</tr>	
							
							<tr>
								<td>City</td> 
								<td><%= value['city'] %></td>
							</tr>
							
							<tr>
								<td>State</td> 
								<td><%= value['state'] %></td>
							</tr>
							
							<tr>
								<td>Country</td> 
								<td><%= value['country'] %></td>
							</tr>
							<tr>
								<td>Zip</td> 
								<td><%= value['zip'] %></td>
							</tr>
							<tr>
								<td>Mobile</td> 
								<td><%= value['mobile'] %></td>
							</tr>		

						<table>
					<% end %>
				<% end %>		
			</div>
		</td>
	</tr>	
</table>
