
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

<script>
	$(function() {
	$( "input[type=submit] , button" )
	.button()
	.click(function( event ) {
		});
	});
</script>
<script>
	$(document).ready(function(){
		$("button").click(function() {
			
			var skill 			 	 = 		document.getElementById('certificate');
			var description		 	 = 		document.getElementById('description');
			var id				 	 = 		document.getElementById('add');


			var skilllen 		 	 = 		skill.value.length;
			var descriptionlen 	 	 = 		description.value.length;
			
			if (skilllen == 0 || descriptionlen == 0)
			{
				alert("some field is empty");  
				return false;
			}
				
			else
			{
	
				  var skills 		 	 = 		skill.value;
				  var descriptions	 	 = 		description.value;
				  var id 	 			 = 		id.value;
				  $.ajax({ 
						  type: 'GET', 
						  url: '/jobseeker/myEg', 
						  data: {'eg' : skills, 'description':descriptions, 'id':id},
						  success: function(data) 
							{
								self.close();
								opener.location.reload(true);
							} 
						});				
			}
		});
	});	
</script>
<script>
$(document).ready(function(){
    $( "#wrapHeader" ).hide();
    $( "#wrapFooter" ).hide();
});
</script>

</head>

<body>

<!-- MAIN CONTENT -->
<div id="wrapContent" class="clearfix">



<!-- ONE COLUMN -->
<div id="oneCol" class="clearfix">
	<!-- Insert jQuery tabs here if applicable -->
	<div class="content">
		<table width="100%">
			<tbody>
				<tr>
					<td>
						<%= form_tag(:action => "myExamples") do  %>	
							<%= render 'formMyExamples' %>
						<%end%>
					</td>				
				</tr>
			</tbody>
		</table>
	</div>
</div>
<!-- END ONE COLUMN -->


</div>
<!-- END MAIN CONTENT -->

</body>
</html>



