<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>Employee</title>
	<script>
	$(document).ready(function(){

		$( "#signout" ).hide();
	});
	</script>
</head>	


<body>

<!-- MAIN CONTENT -->
<div id="wrapContent" class="clearfix">

<!-- BREADCRUMBS -->
<div id="wrapBreadcrumbs" class="clearfix">
	<div id="breadcrumbs">
		<ul>
			<li><a href='/home/homePage'>Home &raquo;</a></li>
			<li><a href='/jobseeker/login'>Login</a></li> <!-- Omit the &raquo; on the last <li> item in order to hide the arrows -->
		</ul>
	</div>
</div>
<!-- END BREADCRUMBS -->

<!-- ONE COLUMN -->
<div id="oneCol" class="clearfix">
	<!-- Insert jQuery tabs here if applicable -->
	<div class="content">
		<table width="100%">
			<tbody>
				<tr>
					<td>
						<%= form_tag(:action => "login") do  %>	
							<%= render 'formLogin' %>
						<% end %>
					</td>					
				</tr>
			</tbody>
		</table>
	</div>
</div>
<!-- END ONE COLUMN -->


</div>
<!-- END MAIN CONTENT -->

</body>
</html>
